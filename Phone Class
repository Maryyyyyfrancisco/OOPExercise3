package composition;

public class Phone {
    private String colour;
    private String power;

   
    public Phone(String colour){
        this.colour = colour;
        power  = new power();
    }

    public Phone(String colour, String power) {
        this.colour = colour;
        this.power = power;
    }

    
    public String getColour() {
        return colour;
    }

    
    public void setColour(String colour) {
        this.colour = colour;
    }

    public void setPower(String power) {
        this.power = power;
    }

    
    public String open() {
        return power.openPower();
    }
}
